;============================================================
; [Mode] is an session identifier
; "Mode 0" is a identifier and can have Any Name
; "Level Mode" is a session assigned to "Mode 0"
;============================================================
[Modes]
  begin   
    Mode 0 = Level Mode
  end;
;------------------------------------------------------------
[Level Mode]
  begin
    Edit Mode =   Level    
    Tiles  =      Kosinski
    Blocks =      Kosinski
    Chunks =      kosinski
    Layout =      Kosinski
    Chunk Mode =  128x128
    Bg Mode =     Interleaved
    Respaw Mode = Y HiBit
    Ring Mode =   Y HiBit
    Object Path = %Project%\Editor
  end;
;============================================================
